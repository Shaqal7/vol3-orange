<%= simple_form_for @place do |f| %>
  <%= f.input :name %>
  <%= f.input :country, priority: ["AU", "NZ", "GB", "US"], label: false %>
  <%= f.input :start_date, as: :date, start_year: 2000,
    end_year: Date.today.year + 5 %>
  <%= f.input :end_date, as: :date, start_year: 2000,
    end_year: Date.today.year + 5 %>
  <%= f.button :submit %>
<% end %>
