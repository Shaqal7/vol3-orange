<%= simple_form_for [@trip, @place, @attraction] do |a| %>
  <%= a.input :name %>
  <%= a.input :start_date, as: :string, input_html: { type: :date } %>
  <%= a.input :end_date, as: :string, input_html: { type: :date } %>
  <%= a.input :cost %>
  <%= a.button :submit, class: 'btn btn-success btn-block' %>
<% end %>
